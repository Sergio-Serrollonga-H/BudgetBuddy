{
  "expo": {
    "name": "Budget Buddy",
    "slug": "BudgetBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/moneyTracker.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/moneyTracker.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sergio2822.BudgetBuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/moneyTracker.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sergio2822.BudgetBuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/moneyTracker.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0593b446-b4b9-43ce-a45b-6cba55809491"
      }
    }
  }
}
